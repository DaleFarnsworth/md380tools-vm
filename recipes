#!/bin/bash 
. ~/md380tools-vm/strings

clear
echo -e "                ${WHITE}${ZPROJ} - ${ZRECIPS}${NC}"
echo -e ""
echo -e " ${WHITE}${ZRAD10} / ${ZRAD11}${NC}"
echo -e ""
echo -e "\t${YELLOW}${ZCOM1}  ${ZCOM2}  ${ZCOM4}${NC}"
echo -e ""
echo -e " ${WHITE}${ZRAD20} / ${ZRAD12}${NC}"
echo -e ""
echo -e "\t${YELLOW}${ZCOM1}  ${ZCOM3}  ${ZCOM4}${NC}"
echo -e ""
echo -e " ${WHITE}${ZCOM12}${NC}"
echo -e ""
echo -e "\t${YELLOW}${ZCOM10}  ${ZCOM4}${NC}"
echo -e ""
echo -e " ${WHITE}${ZCOM16}${NC}"
echo -e ""
echo -e "\t${YELLOW}${ZCOM17}${NC} ${BLUE}${ZCOM19}${NC}"
echo -e "\t${YELLOW}${ZCOM18}${NC} ${BLUE}${ZCOM19}${NC} ${ZCOM20} ${YELLOW}${ZCOM18}${NC} ${ZCOM21}"
echo -e "\t${YELLOW}${ZCOM18} X ( X => 1 to 9)${NC} ${ZCOM46}"
echo -e "\t${YELLOW}${ZCOM18} stable${NC} ${ZCOM45}"
echo -e ""
echo -e "\t${YELLOW}${ZCOM8}${NC}   -- ${ZCOM22}"
echo -e -n "\t${YELLOW}menu${NC}     -- Back to main menu "
if [ -f /etc/motd.save ]; then
  echo -e " (${ZCOM20} ${WHITE}Ctrl+D${NC})"
else
  echo -e ""
fi
echo -e ""
